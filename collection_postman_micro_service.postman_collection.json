				{
					"name": "DELETE All Cards (via tests)",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"let cards = pm.response.json();",
									"if (Array.isArray(cards)) {",
									"    cards.forEach(card => {",
									"        pm.sendRequest({",
									"            url: `http://localhost:8082/api/cards/${card.id}`,",
									"            method: 'DELETE'",
									"        }, function (err, res) {",
									"            console.log(`Deleted card: ${card.id}`);",
									"        });",
									"    });",
									"} else {",
									"    console.log('No cards to delete.');",
									"}"
								],
								"type": "text/javascript"
							}
